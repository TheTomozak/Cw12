
@model IEnumerable<Cw12.Models.Doctor>


<h1>Lista doktorów</h1>
<p>
    <table>
        
        @foreach (var doctor in Model)
        {
            <tr>
                <td>
                    @doctor.FirstName

                </td>
                
                <td>
                    
                    @doctor.LastName
                </td>
                
                <td>
                    
                    @doctor.Email

                </td>

            </tr>
        }

    </table>
</p>
